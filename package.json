{
  "name": "electronshoppinglistapp",
  "version": "1.0.0",
  "description": "Shopping List Application ",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ElectronShoppingListApp\"",    
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mohitbhavsar707/ElectronShoppingListApp.git"
  },
  "author": "Mohit Bhavsar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mohitbhavsar707/ElectronShoppingListApp/issues"
  },
  "homepage": "https://github.com/Mohitbhavsar707/ElectronShoppingListApp#readme",
  "dependencies": {
    "electron": "^22.0.2"
  },
  "devDependencies": {
    "electron-packager": "^17.1.1"
  }
}
